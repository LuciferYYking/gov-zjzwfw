<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>JavaLegalWebDemo</display-name>
 <servlet>
    <display-name>LegalServlet</display-name>
    <servlet-name>LegalServlet</servlet-name>
    <servlet-class>cn.hangzhou.demo.servlet.LegalServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LegalServlet</servlet-name>
    <url-pattern>/LegalServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>demo.html</welcome-file>
  </welcome-file-list>
</web-app>